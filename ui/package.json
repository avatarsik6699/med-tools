{
  "name": "ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/avatarsik6699/med-tools"
  },
  "scripts": {
    "dev": "vite",
    "dev:local": "vite --host --mode local",
    "dev:proxy": "vite --host --mode proxy",
    "dev:network": "vite --host 0.0.0.0",
    "build": "tsc -b && vite build",
    "build:analyze": "vite build --mode analyze",
    "build:preview": "vite build --mode preview",
    "build:production": "vite build --mode production",
    "analyze": "vite-bundle-analyzer",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 1000",
    "type-check": "tsc --noEmit",
    "clean": "npx rimraf dist node_modules/.vite",
    "clean:all": "npx rimraf dist node_modules package-lock.json",
    "reinstall": "npm run clean:all && npm install",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "deps:check": "npm-check-updates",
    "deps:update": "npm-check-updates -u",
    "deps:install": "npm install",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "prepare": "husky"
  },
  "dependencies": {
    "@mantine/core": "8.2.8",
    "@mantine/dates": "8.2.8",
    "@mantine/form": "8.2.8",
    "@mantine/hooks": "8.2.8",
    "@tanstack/react-query": "5.86.0",
    "@tanstack/react-router": "1.131.35",
    "@xyflow/react": "12.8.4",
    "axios": "1.11.0",
    "dayjs": "1.11.18",
    "eruda": "3.4.3",
    "husky": "9.1.7",
    "mobx": "6.13.7",
    "mobx-react-lite": "4.1.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-error-boundary": "6.0.0",
    "react-icons": "5.5.0",
    "react-query-kit": "3.3.2",
    "zod": "4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "9.34.0",
    "@types/node": "24.3.1",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.2",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-format": "1.0.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "gh-pages": "6.3.0",
    "globals": "16.3.0",
    "postcss": "8.5.6",
    "postcss-preset-mantine": "1.18.0",
    "postcss-simple-vars": "7.0.1",
    "typescript": "5.9.2",
    "typescript-eslint": "8.42.0",
    "vite": "7.1.4"
  }
}
